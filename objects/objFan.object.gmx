            <string>///AI
if (state == "FAN_MOVE1") //Start moving
{
    if (x &lt; objCameraHR.x) &amp;&amp; (hspeed = 0) //Move if on screen
    {
        hspeed = -1; 
    }
    else hspeed = -1;
    
    if (x &lt; view_xview+global.screen_width/2-25)
    {
        state = "FAN_MOVE2"
    }
}

if (state == "FAN_MOVE2") //Move backwards diagonally
{
    hspeed = 2;
    if (y &lt; view_hview/2)
    {
        vspeed = 2;
        state = "FAN_MOVE3"
    }
    else if (y &gt; view_hview/2)
    {
        vspeed = -2;
        state = "FAN_MOVE3"
    }
}

if (state == "FAN_MOVE3") //Retreat
{
    if instance_exists(objVicViper)
    {
        if (vspeed == 2) &amp;&amp; (y &gt;= objVicViper.y) 
        {
            vspeed = 0;
            hspeed = 4;
        }
        else if (vspeed == -2) &amp;&amp; (y &lt;= objVicViper.y) 
        {
            vspeed = 0;
            hspeed = 4;
        }
    }
    else
    {
        if (vspeed == 2) &amp;&amp; (y &gt;= view_hview/2) 
        {
            vspeed = 0;
            hspeed = 4;
        }
        if (vspeed == -2) &amp;&amp; (y &lt;= view_hview/2) 
        {
            vspeed = 0;
            hspeed = 4;
        }
    }
}

//Destroy if done &amp; offscreen
if (state == "FAN_MOVE3") &amp;&amp; (x &gt; objCameraHR.x)
{
    instance_destroy();
}
</string>
